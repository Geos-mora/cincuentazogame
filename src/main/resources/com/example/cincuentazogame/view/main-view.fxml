<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="400.0" prefWidth="607.0" stylesheets="@main-styles.css" xmlns="http://javafx.com/javafx/24.0.1"   xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cincuentazogame.controller.MainController">
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" styleClass="header" BorderPane.alignment="CENTER">
            <Label text="CINCUENTAZO" />
      </HBox>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="mainBody" />
         <VBox styleClass="containerSeleccionarBots">
            <children>
               <Label text="elige el numero de de bots a jugar " />
                     <HBox prefHeight="14.0" prefWidth="607.0" styleClass="containerSeleccionarBots">
                           <Button mnemonicParsing="false" text="1" />
                           <Button mnemonicParsing="false" text="2" />
                           <Button mnemonicParsing="false" text="3" />
                     </HBox>
            </children>
         </VBox>

      </VBox>
   </center>
    <bottom>
        <HBox prefHeight="49.0" prefWidth="607.0" styleClass="footer" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="JUGAR" />
         </children></HBox>
    </bottom>
</BorderPane>
